[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = VN
ST = Hanoi
L  = Hanoi
O  = MyOrg
OU = Dev
CN = HOSTNAME

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = HOSTNAME
DNS.2 = localhost
IP.1  = 127.0.0.1
